{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": [
            "es2019"
        ],
        "sourceMap": true,
        "outDir": "dist",
        "rootDir": "src",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true
    }
}
